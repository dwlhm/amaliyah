{
  "name": "amaliyah",
  "version": "1.1.0",
  "description": "webapp untuk memudahkan anda membaca panduan amalan TQN PP Suryalaya Sirnarasa",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "dev": "node server",
    "start": "serve ./functions",
    "start:ui": "concurrently --kill-others-on-fail \"pnpm start:css\" \"pnpm start:sb\"",
    "build": "concurrently --kill-others-on-fail -n w: \"pnpm:build:*\"",
    "build:client": "vite build --outDir functions/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir functions/server",
    "start:sb": "start-storybook -p 6006",
    "build:sb": "build-storybook",
    "start:css": "tailwindcss -o ./public/index.css --watch",
    "build:css": "tailwindcss -o ./public/index.css"
  },
  "keywords": [],
  "author": "dwlhm",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/html": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "compression": "^1.7.4",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "postcss": "^8.4.20",
    "serve": "^14.1.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-pwa": "^0.14.0",
    "vite-plugin-solid": "^2.5.0"
  },
  "dependencies": {
    "@solidjs/router": "^0.6.0",
    "solid-js": "^1.6.6",
    "solid-meta": "^0.28.1"
  }
}
